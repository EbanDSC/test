{
    "executionRoleArn": "arn:aws:iam::334194130763:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "webapp",
            "image": "334194130763.dkr.ecr.us-east-1.amazonaws.com/webapp",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "node-js-app"
}